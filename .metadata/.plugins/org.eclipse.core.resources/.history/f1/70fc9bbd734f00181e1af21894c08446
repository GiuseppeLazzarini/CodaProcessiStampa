
public class MainClass {

	public static void main(String[] args) 
	{
		// TODO Auto-generated method stub
		
		ProcessoStampa p1=new ProcessoStampa("pc1","word","ciao");
		ProcessoStampa p2=new ProcessoStampa("pc1","excel","io");
		ProcessoStampa p3=new ProcessoStampa("pc4","excel","tu");
		ProcessoStampa p4=new ProcessoStampa("pc5","powerpoint","basta");
		
		CodaStampa s1=new CodaStampa();
		
		s1.enqueue(p1);
		s1.enqueue(p2);
		s1.enqueue(p3);
		s1.enqueue(p4);
		
		System.out.println(s1.toString());
		
		try 
		{
			s1.dequeue();
			s1.dequeue();
		} 
		catch (StampaException e) 
		{
			System.out.println(e.toString());
		}
		
		System.out.println(s1.toString());
	}

}


public class Nodo 
{
	private ProcessoStampa info;
	private Nodo link;
	
	public Nodo (Invitato persona)
	{
		setInfo(persona);
		setLink(null);
	}

	public Invitato getInfo() {
		return info;
	}

	public void setInfo(Invitato info) {
		this.info = info;
	}

	public Nodo getLink() {
		return link;
	}

	public void setLink(Nodo link) {
		this.link = link;
	}
}
